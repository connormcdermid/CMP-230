    20                                  ; SUBROUTINE: fib
    21                                  ; INPUTS: Quantity
    22                                  ; OUTPUTS: Requested numbers in RAX
    23                                  fib:
    24                                  	; subroutine prologue
    25 00000000 55                      	push	rbp	; save caller base pointer
    26 00000001 4889E5                  	mov	rbp, rsp; new base pointer
    27 00000004 4883EC18                	sub	rsp, 3*8; allocate 2 local vars on stack
    28 00000008 53                      	push	rbx	; save caller regs
    29 00000009 51                      	push	rcx	; for use as working regs
    30                                  	
    31                                  	; subroutine body
    32 0000000A 488B4510                	mov	rax, [rbp + 2*8]; retrieve parameter
    33 0000000E 4883F801                	cmp	rax, 1		; if n = 1
    34 00000012 7E34                    	jle	return		; return from subroutine
    35                                  				; else, continue
    36 00000014 BB01000000              	mov	rbx, 1
    37 00000019 B902000000              	mov	rcx, 2
    38 0000001E 50                      	push 	rax	; preserve original value of RAX
    39 0000001F 4829D8                  	sub	rax, rbx	; n - 1
    40 00000022 4889C3                  	mov	rbx, rax	; rbx should hold n - 1 value
    41 00000025 58                      	pop	rax		; restore value of RAX
    42 00000026 4829C8                  	sub	rax, rcx	; n - 2
    43 00000029 4889C1                  	mov	rcx, rax	; rcx should hold n - 2 value
    44 0000002C 53                      	push	rbx		; pass n - 1 to fib()
    45 0000002D E8CEFFFFFF              	call	fib
    46 00000032 5B                      	pop	rbx
    47 00000033 4889C3                  	mov	rbx, rax	; fib(n - 1) now in RBX
    48 00000036 51                      	push	rcx		; pass n - 2 to fib()
    49 00000037 E8C4FFFFFF              	call	fib
    50 0000003C 59                      	pop 	rcx
    51 0000003D 4889C1                  	mov	rcx, rax	; fib(n - 2) now in RCX
    52                                  				; yeah, yeah, I could just use RAX, but this is more readable
    53                                  	
    54 00000040 4801CB                  	add	rbx, rcx	; fib(n - 1) + fib(n - 2)
    55 00000043 4889D8                  	mov	rax, rbx	; move result of fib function to RAX for return
    56 00000046 EB00                    	jmp	return
    57                                  	
    58                                  return	equ	$
    59                                  	; subroutine epilogue
    60 00000048 59                      	pop	rcx		; restore caller regs
    61 00000049 5B                      	pop	rbx	
    62 0000004A 4883C418                	add	rsp, 3*8	; clear stack of local var allocations
    63 0000004E 4889EC                  	mov	rsp, rbp	; restore caller stack pointer
    64 00000051 5D                      	pop	rbp	; restore caller base pointer
    65 00000052 C3                      	ret	; return from subroutine with result in RAX
